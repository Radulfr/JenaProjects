      dc:relation "http://www.gijon.es/data/images/f0/f2/28705/169x89_v1_31927_81812008135557.jpg" ;
      geo:name "Apartamentos Urbanos Moros 41" ;
      cal:street-address "C/ Moros, 41 33206 Gij&#243;n" ;
      cal:tel "+34 660 515 859" .

